{
     "version": 2,
     "buildCommand": "cd vercel-app && npm install",
     "installCommand": "cd vercel-app && npm install",
     "rewrites": [
       {
         "source": "/api/(.*)",
         "destination": "/vercel-app/api/$1"
       },
       {
         "source": "/(.*)",
         "destination": "/vercel-app/public/$1"
       }
     ]
   }
